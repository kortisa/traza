--

CREATE VIEW vi_destination AS
 SELECT a.code,
    a.name_destination,
    b.area_code,
    b.name,
    a.currency,
    a.constant
   FROM ta_destination a,
    ta_area_code b
  WHERE ((a.area_code)::text = (b.area_code)::text);


ALTER TABLE vi_destination OWNER TO postgres;

