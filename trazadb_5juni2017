--

CREATE VIEW vi_employee AS
 SELECT a.nik,
    a.name,
    a.email,
    a.telegramid,
    b.deptid,
    b.deptname,
    c.postid,
    c.postname,
    a.grade
   FROM ta_employee a,
    ta_department b,
    ta_position c
  WHERE (((a.deptid)::bpchar = (b.deptid)::bpchar) AND ((a.postid)::bpchar = (c.postid)::bpchar));


ALTER TABLE vi_employee OWNER TO postgres;

