    (
        now(),
        TG_OP,
        new.idpermintaan,
        new.nikpengaju,
        new.nikpejalan,
        new.lokasipergi,
        new.lokasipulang,
        new.waktupermintaan
    );
    RETURN NEW;
ELSIF TG_OP='DELETE' then
    INSERT INTO ta_log_ta_permintaanpergipulang
    (
        log_time,
        log_action,
        idpermintaan,
  		nikpengaju,
        nikpejalan,
        lokasipergi,
  		lokasipulang,
        waktupermintaan
    )
    VALUES
    (
        now(),
        TG_OP,
        old.idpermintaan,
        old.nikpengaju,
        old.nikpejalan,
        old.lokasipergi,
        old.lokasipulang,
        old.waktupermintaan
    );
    RETURN OLD;
END IF;
